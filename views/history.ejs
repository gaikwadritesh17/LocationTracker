<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>History</title>
</head>
<body>
    <h1>Location History</h1>
    <ul>
        <% locations.forEach(location => { %>
            <li><%= location.latitude %>, <%= location.longitude %> at <%= location.timestamp %></li>
        <% }); %>
    </ul>
    <a href="/">Back to Map</a>
</body>
</html>
